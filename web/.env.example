# Supabase Database URLs
# Connection pooler for serverless (use in production)
DATABASE_URL="postgresql://postgres.[YOUR-PROJECT-REF]:[YOUR-PASSWORD]@aws-0-us-east-1.pooler.supabase.com:6543/postgres?pgbouncer=true&connection_limit=1&sslmode=require"

# Direct connection (use for migrations)
DIRECT_URL="postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres?sslmode=require"

# Supabase API Keys
NEXT_PUBLIC_SUPABASE_URL="https://[YOUR-PROJECT-REF].supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key-here"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key-here"

# Authentication
JWT_SECRET="change-this-secret-key-to-something-secure"

# Agent Tokens (comma separated - one per PC)
AGENT_TOKENS="token-pc1-secreto,token-pc2-secreto,token-pc3-secreto,token-pc4-secreto,token-pc5-secreto"
